@{
    ViewBag.Title = "Ingresar Ultimo Tweet De Jugador";
}
<h1>Ingresar Ultimo Tweet De Jugador</h1>

<form method="post" action="/jugadores/IngresarUltimoTweetDeJugador">

    <div>
        Usuario Twiter: <input type="text" name="usuarioTwiter" value="@ViewBag.UsuarioTwiter" />
    </div>
    <div>
        Tweet: <input type="text" name="tweet" value="@ViewBag.Tweet" />
    </div>

    <input type="submit" value="Ingresar" />
</form>